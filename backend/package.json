{
  "name": "hjb-backend",
  "version": "0.0.0",
  "description": "HiJackBox Backend",
  "type": "commonjs",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "~27.4",
    "@types/node": "~16.11",
    "@typescript-eslint/eslint-plugin": "~5.14",
    "@typescript-eslint/parser": "~5.14",
    "eslint": "~8.11",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.1",
    "jest": "~27.5",
    "nodemon": "^2.0.16",
    "prettier": "~2.5",
    "rimraf": "~3.0",
    "ts-jest": "~27.1",
    "tsutils": "~3.21",
    "typescript": "~4.6"
  },
  "scripts": {
    "start": "nodemon --experimental-modules build/src/main.js",
    "start:dev": "node --experimental-modules src/main.js",
    "start:ts": "ts-node src/main",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "resolutions": {
    "hjb-global": "file:../global"
  },
  "author": "Jakub Synowiec <jsynowiec@users.noreply.github.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.1",
    "hjb-global": "file:../global",
    "socket.io": "^4.5.0",
    "ts-node": "^10.7.0",
    "tslib": "~2.3.1",
    "yarn": "^1.22.18"
  }
}
